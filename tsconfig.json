{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./dist",
    "rootDir": "./src",
    "types": ["chai", "mocha", "@hapi/joi", "passport", "passport-http","supertest"]
  },
  "include": ["src/**/*.ts"],
  "exclude": ["node_modules"]
}
